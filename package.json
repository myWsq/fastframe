{
  "name": "fastframe",
  "version": "1.0.0",
  "main": "dist/fastframe.js",
  "types": "dist/fastframe.d.ts",
  "files": [
    "dist",
    "api-references",
    "example"
  ],
  "scripts": {
    "dev": "webpack serve -c webpack.config.dev.js",
    "bundle": "rimraf dist && webpack build",
    "api": "rimraf temp && tsc --emitDeclarationOnly && api-extractor run && api-documenter markdown --input-folder temp --output-folder api-references",
    "build": "npm run bundle && npm run api"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "mitt": "^3.0.0",
    "nanoid": "^3.1.23",
    "fast-memoize": "^2.5.2",
    "@folmejs/core": "^2.4.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.23",
    "@microsoft/api-extractor": "^7.16.1",
    "@types/webpack": "^5.28.0",
    "esbuild-loader": "^2.13.1",
    "html-webpack-plugin": "^5.3.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "worker-loader": "^3.0.8"
  }
}